{
  "name": "mpv-subtitleminer",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview --port 5173",
    "lint": "node ./node_modules/eslint/bin/eslint.js \"src\" --ext .ts,.vue",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier \"src/**/*.{ts,vue,css,scss,md}\" --check",
    "format:fix": "prettier \"src/**/*.{ts,vue,css,scss,md}\" --write"
  },
  "dependencies": {
    "vue": "^3.5.24"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "@eslint/js": "^9.39.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^10.7.0",
    "prettier": "^3.4.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.53.0",
    "vite": "^7.2.4",
    "vite-plugin-singlefile": "^2.3.0",
    "vue-tsc": "^3.1.4",
    "vue-eslint-parser": "^10.2.0"
  }
}
